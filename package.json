{
  "name": "kotlin-scp",
  "version": "0.0.0",
  "description": "Kotlin Secure Coding Practices",
  "repository": {
    "type": "git",
    "url": "https://github.com/Checkmarx/Kotlin-SCP"
  },
  "main": "index.js",
  "dependencies": {
    "gitbook-convert": "^1.1.1"
  },
  "devDependencies": {
    "gitbook-cli": "^2.3.2"
  },
  "scripts": {
    "serve": "node_modules/.bin/gitbook serve",
    "build:pdf": "node_modules/.bin/gitbook pdf ./ ./dist/kotlin-secure-coding-practices.pdf",
    "build:epub": "node_modules/.bin/gitbook epub ./ ./dist/kotlin-secure-coding-practices.epub",
    "build:mobi": "node_modules/.bin/gitbook mobi ./ ./dist/kotlin-secure-coding-practices.mobi",
    "build:docx": "ebook-convert ./dist/kotlin-secure-coding-practices.epub ./dist/kotlin-secure-coding-practices.docx",
    "build": "npm run build:pdf; npm run build:epub; npm run build:mobi; npm run build:docx"
  },
  "keywords": [
    "Kotlin",
    "Mobile",
    "Security",
    "Checkmarx"
  ],
  "author": "Checkmarx Security Research Team",
  "license": "CC-BY-SA-4.0)"
}
